<?xml version="1.0"?>
<!DOCTYPE look-and-feel PUBLIC "-//Liferay//DTD Look and Feel 7.1.0//EN" "http://www.liferay.com/dtd/liferay-look-and-feel_7_1_0.dtd">

<look-and-feel>
	<compatibility>
		<version>7.2.0+</version>
	</compatibility>
	<theme id="sample-senna-surface" name="sample-senna-surface">
		<template-extension>ftl</template-extension>
		<settings>
			<setting configurable="true" key="fwd-show-header-search" type="checkbox" value="false" />
			<setting configurable="true" key="fwd-show-breadcrumb" type="checkbox" value="true" />
			<setting configurable="true" key="fwd-show-bottom" type="checkbox" value="false" />
			<setting configurable="true" key="fwd-show-bottom-sitemap" type="checkbox" value="false" />
			<setting configurable="true" key="fwd-show-bottom-webcontent" type="checkbox" value="false" />
			<setting configurable="true" key="fwd-show-logo" type="checkbox" value="false" />
		</settings>
		<portlet-decorator id="barebone" name="Barebone">
			<portlet-decorator-css-class>portlet-barebone</portlet-decorator-css-class>
		</portlet-decorator>
		<portlet-decorator id="borderless" name="Borderless">
			<portlet-decorator-css-class>portlet-borderless</portlet-decorator-css-class>
		</portlet-decorator>
		<portlet-decorator id="decorate" name="Decorate">
			<default-portlet-decorator>true</default-portlet-decorator>
			<portlet-decorator-css-class>portlet-decorate</portlet-decorator-css-class>
		</portlet-decorator>
	</theme>
</look-and-feel>
